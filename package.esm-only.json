{
  "name": "refine-d1",
  "description": "Cloudflare D1 data provider for Refine framework - Build admin panels and CRUD apps with edge D1 databases",
  "version": "3.0.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md", 
    "LICENSE.md",
    "CHANGELOG.md"
  ],
  "author": "Mateus Abelli",
  "keywords": [
    "refine",
    "data provider", 
    "cloudflare",
    "d1",
    "database",
    "edge",
    "workers",
    "esm"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsup --config tsup.esm-only.config.ts --minify",
    "dev": "tsup --config tsup.esm-only.config.ts --watch",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "peerDependencies": {
    "@refinedev/core": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250703.0",
    "@refinedev/core": "^4.57.9",
    "@types/node": "^20.19.4",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^2.1.8"
  }
}
